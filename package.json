{
  "name": "muidev",
  "version": "0.1.1",
  "private": false,
  "type": "module",
  "description": "A modern React UI component library built with Tailwind CSS and Framer Motion. Beautiful, accessible, and customizable components for your next project.",
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwindcss",
    "framer-motion",
    "design-system",
    "react-components",
    "ui-library",
    "kenya",
    "african-tech"
  ],
  "author": "TELVIN TEUM",
  "contributors": [
    "SAMUELDEVLP"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BotCoder254/mui.git"
  },
  "bugs": {
    "url": "https://github.com/BotCoder254/mui/issues"
  },
  "homepage": "https://github.com/BotCoder254/mui#readme",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tailwindcss": ">=3.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.5.1",
    "framer-motion": "^11.14.1",
    "postcss": "^8.4.31",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-icons": "^4.11.0",
    "tailwindcss": ">=3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:lib": "NODE_ENV=production npx rollup -c",
    "build:css": "NODE_ENV=production npx postcss src/index.css -o dist/index.css --config postcss.config.cjs",
    "prepare": "npm run build:lib && npm run build:css",
    "prepublishOnly": "npm test && npm run build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@storybook/addon-docs": "^7.4.5",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.5",
    "@storybook/preset-create-react-app": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-webpack5": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-storybook": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^7.3.3",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.4.5",
    "tailwindcss": "^3.4.17"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "extends": [
          "plugin:storybook/recommended"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
